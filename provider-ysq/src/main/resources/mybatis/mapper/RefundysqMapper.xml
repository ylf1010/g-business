<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jk.dao.RefundysqMapper">

<select id="cha" parameterType="com.jk.util.ztx.ParameUtil" resultType="com.jk.model.Refundysq">
  select * from refund-ysq where 1=1 <include refid="aa"></include>
</select>
  <sql id="aa">
    <if test="refundway!=null and refundway!=''">
      and refundway = #{refundway}
    </if>
    <if test="status!=null and status!=''">
      and status = #{status}
    </if>
    <if test="state!='' and state!=''">
      and state = #{state}
    </if>
    <if test="cause!=null and cause!=''">
      and cause = #{cause}
    </if>
  </sql>
</mapper>